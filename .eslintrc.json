{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/extensions": ["error", "ignorePackages", { "js": "always" }],
    "no-underscore-dangle": [2, { "allow": ["__filename", "__dirname"] }]
  }
}
